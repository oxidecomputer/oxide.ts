{
  "name": "oxide-ts-generator",
  "version": "1.0.0",
  "description": "Minimal TS OpenAPI client generator",
  "main": "index.ts",
  "scripts": {
    "gen": "./gen.sh $(cat ../OMICRON_VERSION) ../client",
    "gen-from": "./gen.sh",
    "postgen": "prettier --parser typescript --write ../client",
    "postgen-from": "npm run postgen",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "tsc": "tsc"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.6",
    "openapi-types": "^12.0.0",
    "prettier": "2.7.1",
    "swagger-parser": "^10.0.3",
    "typescript": "^4.7.4",
    "vitest": "^0.22.0"
  }
}
