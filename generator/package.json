{
  "name": "oxide-ts-generator",
  "version": "1.0.0",
  "description": "Minimal TS OpenAPI client generator",
  "main": "index.ts",
  "scripts": {
    "gen": "tsc && node index.js && rm index.js && npm run gen:orgName && prettier -w ../Api.ts",
    "gen:orgName": "sed -i '' 's/organizationName/orgName/g' ../Api.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.10",
    "prettier": "2.5.1",
    "swagger-parser": "^10.0.3",
    "typescript": "^4.5.4"
  }
}
