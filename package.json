{
  "name": "@oxide/api",
  "version": "0.0.1",
  "description": "The official JavaScript and TypeScript client SDK for interacting with the Oxide API",
  "keywords": [
    "oxide",
    "oxide.ts",
    "oxide computer",
    "oxide sdk"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "gen": "./tools/gen.sh $(cat ./OMICRON_VERSION) ./client",
    "gen-from": "./tools/gen.sh",
    "lint": "eslint .",
    "postgen": "prettier --parser typescript --write ./client",
    "postgen-from": "npm run postgen",
    "pretest": "npm run gen",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "tsc": "tsc"
  },
  "author": "",
  "license": "MPL-2.0",
  "optionalDependencies": {
    "zod": "^3.20"
  },
  "devDependencies": {
    "@types/node": "^18.7.20",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "msw": "^2.0.9",
    "openapi-types": "^12.0.2",
    "prettier": "2.7.1",
    "swagger-parser": "^10.0.3",
    "ts-pattern": "^4.0.5",
    "tsafe": "^1.1.1",
    "tsx": "^4.7.1",
    "type-fest": "^3.1.0",
    "typescript": "^5.2.2",
    "vitest": "^1.4.0",
    "zod": "3.21.1"
  }
}
