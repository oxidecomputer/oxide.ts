{
  "name": "@oxide/api",
  "version": "0.0.1",
  "description": "The official JavaScript and TypeScript client SDK for interacting with the Oxide API",
  "keywords": [
    "oxide",
    "oxide.ts",
    "oxide computer",
    "oxide sdk"
  ],
  "main": "./dist/Api.js",
  "module": "./dist/Api.mjs",
  "types": "./dist/Api.d.ts",
  "exports": {
    ".": {
      "require": "./dist/Api.js",
      "import": "./dist/Api.mjs",
      "types": "./dist/Api.d.ts"
    },
    "./validate": {
      "require": "./dist/validate.js",
      "import": "./dist/validate.mjs",
      "types": "./dist/validate.d.ts"
    }
  },
  "files": [
    "client",
    "dist"
  ],
  "scripts": {
    "build": "tsup client/Api.ts client/validate.ts --format cjs,esm --dts --clean",
    "gen": "./tools/gen.sh $(cat ./OMICRON_VERSION) ./client && npm run build",
    "gen-from": "./tools/gen.sh",
    "postgen": "prettier --parser typescript --write ./client",
    "postgen-from": "npm run postgen",
    "pretest": "npm run gen",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "tsc": "tsc"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.20",
    "esbuild-register": "^3.3.3",
    "happy-dom": "^6.0.4",
    "openapi-types": "^12.0.0",
    "prettier": "2.7.1",
    "swagger-parser": "^10.0.3",
    "ts-pattern": "^4.0.5",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4",
    "vitest": "^0.22.0",
    "zod": "^3.19.1"
  }
}
